{
  "extends": "next/core-web-vitals",
   "rules": {
      "no-console": "warn",
      "prefer-const": "error",
      "quotes": ["error", "single"],
      "jsx-quotes": ["error", "prefer-double"],
      "indent": ["warn", 3],
      "react/jsx-indent": ["warn", 3],
      "max-len": ["error", {
         "code": 120,
         "ignoreComments": true
      }],
      "comma-dangle": ["error", "always-multiline"],
      "semi": ["warn", "always"],
      "no-undef": "off"
   }
}
